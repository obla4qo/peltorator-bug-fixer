{
  "manifest_version": 3,
  "name": "Peltorator bug fixer",
  "version": "1.1",
  "description": "Исправляет ошибку на сайте peltorator",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "Vladimir_Putin@kremlin.ru"
    }
  }
}
